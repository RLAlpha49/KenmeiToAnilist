{
  "name": "Kenmei To Anilist",
  "entryPointStrategy": "expand",
  "entryPoints": ["../src"],
  "out": "../documentation",
  "plugin": [
    "typedoc-theme-hierarchy",
    "typedoc-plugin-missing-exports",
    "typedoc-plugin-rename-defaults",
    "typedoc-plugin-extras",
    "typedoc-plugin-inline-sources"
  ],
  "theme": "hierarchy",
  "tsconfig": "../tsconfig.json",
  "exclude": [
    "**/src/tests/**/*",
    "**/node_modules/**/*",
    "**/node_modules/**/*",
    "**/src/components/ui/*"
  ],
  "skipErrorChecking": true,
  "externalPattern": ["**/node_modules/**"],
  "excludeExternals": true,
  "excludeNotDocumented": false,
  "excludePrivate": true,
  "excludeProtected": true,
  "readme": "none",
  "disableSources": false,
  "includeVersion": true,
  "categoryOrder": ["Module", "*"],
  "cleanOutputDir": true,
  "preserveWatchOutput": true,
  "sort": ["source-order"],
  "placeInternalsInOwningModule": true,
  "hostedBaseUrl": "https://rlalpha49.github.io/KenmeiToAnilist/",
  "useHostedBaseUrlForAbsoluteLinks": true,
  "customTitle": "Kenmei To Anilist",
  "footerTypedocVersion": true,
  "footerLastModified": true,
  "blockTags": [
    "@source",
    "@description",
    "@property",
    "@module",
    "@example",
    "@param",
    "@returns",
    "@throws",
    "@deprecated",
    "@internal",
    "@remarks"
  ]
}
